{
  "navClasses": {
    "type": "attribute",
    "key": "class",
    "value": {
      "type": "group",
      "values": [
        {
          "type": "const",
          "value": "nav"
        },
        {
          "label": "Tab style",
          "type": "select",
          "values": [
            {
              "label": "Tabs",
              "value": "nav-tabs"
            },
            {
              "label": "Pills",
              "value": "nav-pills"
            }
          ]
        },
        {
          "label": "Stacked",
          "type": "switch",
          "on": "nav-stacked"
        },
        {
          "label": "Justified",
          "type": "switch",
          "on": "nav-justified"
        }
      ]
    }
  },
  "section-select": {
    "label": "Layout",
    "type": "section-select",
    "values": [
      {
        "label": "Basic",
        "value": "standard"
      },
      {
        "label": "With dropdown",
        "value": "dropdown"
      }
    ]
  }
}
